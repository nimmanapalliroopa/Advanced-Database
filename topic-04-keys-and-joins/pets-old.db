CREATE TABLE sqlite_sequence(name,seq);

CREATE TABLE pets (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    name TEXT NOT NULL,
    type TEXT NOT NULL,
    age INTEGER,
    owner TEXT
);

